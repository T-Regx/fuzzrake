<table id="artisans" class="table table-striped table-sm table-hover">
    <thead class="thead-dark">
    <tr>
        <th class="text-left">Fursuit maker /&nbsp;studio name</th>
        <th class="hidden">Completeness</th>
        <th class="hidden">Formerly</th>
        <th class="hidden">Intro</th>
        <th class="hidden">Since</th>
        <th class="hidden">Country</th>
        <th class="hidden">State</th>
        <th class="hidden">City</th>
        <th class="text-center toggleable no-sorting">Styles</th>
        <th class="hidden">Other styles</th>
        <th class="text-center toggleable no-sorting default-hidden">Types</th>
        <th class="hidden">Other types</th>
        <th class="text-center toggleable no-sorting default-hidden">Features</th>
        <th class="hidden">Other features</th>
        <th class="hidden">FursuitReview URL</th>
        <th class="hidden">Website URL</th>
        <th class="hidden">FurAffinity URL</th>
        <th class="hidden">DeviantArt URL</th>
        <th class="hidden">Twitter URL</th>
        <th class="hidden">Facebook URL</th>
        <th class="hidden">Tumblr URL</th>
        <th class="hidden">Instagram URL</th>
        <th class="hidden">YouTube URL</th>
        <th class="hidden">CST URL</th>
        <th class="hidden">Queue URL</th>
        <th class="hidden">Prices URL</th>
        <th class="hidden">FAQ URL</th>
        <th class="hidden">Other URLs</th>
        <th class="hidden">Notes</th>
        <th class="hidden">Payment plans</th>
        <th class="hidden">Production model</th>
        <th class="hidden">Species does</th>
        <th class="hidden">Species doesn't</th>
        <th class="hidden">Languages</th>
        <th class="hidden">Maker ID</th>
        <th class="text-center toggleable">Commissions</th>
        <th class="hidden">Commissions last check</th>
        <th class="text-right no-sorting">Links</th>
    </tr>
    </thead>

    <tbody>
    {% for artisan in artisans %}
        <tr class="fursuit-maker">
            {%- spaceless %}
                <td class="name" data-toggle="modal" data-target="#artisanDetailsModal">
                    {{- artisan.name -}}
                </td>
                <td class="hidden">
                    {{- artisan.completeness -}}
                </td>
                <td class="hidden">
                    {{- artisan.formerly -}}
                </td>
                <td class="hidden">
                    {{- artisan.intro -}}
                </td>
                <td class="hidden">
                    {{- artisan.since -}}
                </td>
                <td class="hidden">
                    {{- artisan.country -}}
                </td>
                <td class="hidden">
                    {{- artisan.state -}}
                </td>
                <td class="hidden">
                    {{- artisan.city -}}
                </td>
                <td class="styles" data-toggle="modal" data-target="#artisanDetailsModal">
                    {{- artisan.styles -}}
                </td>
                <td class="hidden">
                    {{- artisan.otherStyles -}}
                </td>
                <td class="types" data-toggle="modal" data-target="#artisanDetailsModal">
                    {{- artisan.types -}}
                </td>
                <td class="hidden">
                    {{- artisan.otherTypes -}}
                </td>
                <td class="features" data-toggle="modal" data-target="#artisanDetailsModal">
                    {{- artisan.features -}}
                </td>
                <td class="hidden">
                    {{- artisan.otherFeatures -}}
                </td>
                <td class="hidden">
                    {{- artisan.fursuitReviewUrl -}}
                </td>
                <td class="hidden">
                    {{- artisan.websiteUrl -}}
                </td>
                <td class="hidden">
                    {{- artisan.furAffinityUrl -}}
                </td>
                <td class="hidden">
                    {{- artisan.deviantArtUrl -}}
                </td>
                <td class="hidden">
                    {{- artisan.twitterUrl -}}
                </td>
                <td class="hidden">
                    {{- artisan.facebookUrl -}}
                </td>
                <td class="hidden">
                    {{- artisan.tumblrUrl -}}
                </td>
                <td class="hidden">
                    {{- artisan.instagramUrl -}}
                </td>
                <td class="hidden">
                    {{- artisan.youtubeUrl -}}
                </td>
                <td class="hidden">
                    {{- artisan.commisionsQuotesCheckUrl -}}
                </td>
                <td class="hidden">
                    {{- artisan.queueUrl -}}
                </td>
                <td class="hidden">
                    {{- artisan.pricesUrl -}}
                </td>
                <td class="hidden">
                    {{- artisan.faqUrl -}}
                </td>
                <td class="hidden">
                    {{- artisan.otherUrls -}}
                </td>
                <td class="hidden">
                    {{- artisan.notes -}}
                </td>
                <td class="hidden">
                    {{- artisan.paymentPlans -}}
                </td>
                <td class="hidden">
                    {{- artisan.productionModel -}}
                </td>
                <td class="hidden">
                    {{- artisan.speciesDoes -}}
                </td>
                <td class="hidden">
                    {{- artisan.speciesDoesnt -}}
                </td>
                <td class="hidden">
                    {{- artisan.languages -}}
                </td>
                <td class="hidden">
                    {{- artisan.makerId -}}
                </td>
                <td class="commissions" data-toggle="modal" data-target="#artisanDetailsModal">
                    {{- artisan.areCommissionsOpen is null ? '' : (artisan.areCommissionsOpen ? 'open' : 'closed') -}}
                </td>
                <td class="hidden">
                    {{- artisan.commissionsQuotesLastCheck|date('Y-m-d H:i') -}}
                </td>
                <td class="links"></td>
            {% endspaceless -%}
        </tr>
    {% endfor %}
    </tbody>
</table>
